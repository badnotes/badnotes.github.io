---
import BaseHead from '../components/Head/BaseHead.astro';
import Nav from '../components/Nav.astro';
import HomeHeader from '../components/HomeHeader.astro';
import Footer from '../components/Footer/Footer.astro'
import SearchInput from '../components/SearchInput.astro'

let pageTitle = 'Search';
let pageDescription = 'Search all Articles';
let seoTitle = 'Search | Fyme';
let seoDescription = '';
---

<html lang="en">
  <head>
    <BaseHead title={seoTitle} description={seoDescription} />
  </head>

  <body class="bg-white text-black font-body leading-normal personality-casual">
    <Nav />
    <main class="py-12 lg:py-20">
      <section class="max-w-3xl mx-auto py-6 lg:py-12 content">
		<p>
			My website list, about Some work samples and problem records. 
		</p>

		<h2 class="title mt-4">Here are my built-in features:</h2>
		<ul class="list-inside list-disc">
			<li>Looks great on any device</li>
			<li>Fully Responsive</li>
			<li>Comes light and dark mode</li>
			<li>Easy to customize and add content</li>
			<li>Built with tailwindcss</li>
			<li>Pin posts and projects</li>
		</ul>
		<p class="mt-4">
			Thanks!
		</p>
	  </section>
    </main>
    <Footer />
  </body>
</html>
